{
  "name": "timesync",
  "version": "0.0.0",
  "description": "TimeSync time tracker implemented in js",
  "main": "app.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node ./dist/app.js",
    "compile": "babel src --out-dir dist",
    "devel": "nodemon --exec babel-node -- ./src/app.js",
    "migrations": "knex migrate:latest",
    "recreate": "rm dev.sqlite3 && knex migrate:latest",
    "linter": "jshint ./src ./tests ./scripts && eslint ./src ./tests ./scripts",
    "fixtures": "node --harmony ./scripts/load_fixtures.js",
    "test": "NODE_ENV=mocha TIMESYNC_AUTH_MODULES='[\"ldap\",\"password\"]' TIMESYNC_LDAP_URL=ldap://localhost:1389 TIMESYNC_LDAP_SEARCH_BASE=ou=passport-ldapauth PORT=8851 DEBUG=true mocha --compilers js:babel/register tests",
    "latte": "sh ./scripts/latte.sh",
    "coverage": "NODE_ENV=mocha TIMESYNC_AUTH_MODULES='[\"ldap\",\"password\"]' TIMESYNC_LDAP_URL=ldap://localhost:1389 TIMESYNC_LDAP_SEARCH_BASE=ou=passport-ldapauth PORT=8851 DEBUG=true node --harmony ./node_modules/istanbul/lib/cli.js cover _mocha -- --compilers js:babel/register tests",
    "create-account": "node --harmony ./scripts/create-account.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/osuosl/timesync-node.git"
  },
  "keywords": [
    "time",
    "tracker"
  ],
  "author": "OSU Open Source Lab <support@osuosl.org>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/osuosl/timesync-node/issues"
  },
  "dependencies": {
    "babel-eslint": "^4.0.10",
    "bcrypt": "^0.8.4",
    "body-parser": "^1.12.3",
    "eslint": "^1.2.0",
    "eslint-config-airbnb": "0.0.8",
    "eslint-plugin-react": "^3.2.3",
    "express": "^4.12.3",
    "knex": "^0.8.6",
    "ldapjs": "mcavage/node-ldapjs",
    "passport": "^0.3.0",
    "passport-ldapauth": "0.3.0",
    "passport-local": "^1.0.0",
    "pg": "^4.4.1",
    "prompt": "^0.2.14",
    "sqlite3": "^3.0.8",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "chai": "^3.2.0",
    "chai-passport-strategy": "^0.2.0",
    "istanbul": "^0.3.17",
    "jshint": "^2.8.0",
    "mocha": "^2.3.2",
    "nodemon": "^1.4.0",
    "request": "^2.55.0",
    "sql-fixtures": "^0.11.0"
  }
}
